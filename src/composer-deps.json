{
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"optimize-autoloader" : true,
		"platform" :
		{
			"php": "8.1"
		},
		"allow-plugins": {
			"composer/installers": true,
			"wpify/scoper": true
		}
	},
	"repositories": [
		{
		"url": "git@github.com:bob-moore/wp-framework-core.git",
				"type": "git"
		}
	],
	"require": {
		"devkit/wp-framework-core": "dev-main",
		"timber/timber": "^2.0"
	}
}
