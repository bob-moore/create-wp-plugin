{
	"name": "devkit_plugin",
	"version": "4.0.1",
	"description": "custom plugin",
	"author": "AUTHOR_NAME <bob.moore@midwestfamilymadison.com>",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"mu-plugin",
		"project",
		"wordpress"
	],
	"homepage": "https://www.midwestfamilymadison.com",
	"repository": "https://github.com/MDMDevOps/{PROJECT}",
	"bugs": "https://github.com/MDMDevOps/{PROJECT}/issues",
	"devDependencies": {
		"@wordpress/scripts": "^26.17.0",
		"autoprefixer": "^10.4.14",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^11.0.0",
		"cssnano": "^6.0.1",
		"mini-css-extract-plugin": "^2.7.6",
		"postcss": "^8.4.24",
		"postcss-assets": "^6.0.0",
		"postcss-nested": "^6.0.1",
		"postcss-pxtorem": "^6.0.0",
		"pxtorem": "^1.0.0",
		"sass": "^1.63.6"
	},
	"scripts": {
		"build": "npm run lint:css && npm run lint:js && npm run compile",
		"compile": "npm run compile:blocks && npm run compile:assets",
		"watch": "npm run watch:blocks & npm run watch:assets",
		"watch:hot": "npm run watch:blocks & npm run watch:assets -- --hot",
		"compile:blocks": "wp-scripts build --webpack-src-dir=./blocks --webpack-copy-php --config blocks.config.js",
		"watch:blocks": "wp-scripts start --webpack-src-dir=./blocks --webpack-copy-php --config blocks.config.js",
		"compile:assets": "wp-scripts build --webpack-src-dir=./",
		"watch:assets": "wp-scripts start --webpack-src-dir=./",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses --prod --gpl2",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style './**/*.scss'",
		"fix:css": "wp-scripts lint-style './**/*.scss' --fix",
		"lint:js": "wp-scripts lint-js",
		"fix:js": "wp-scripts lint-js --fix",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js"
	},
	"browserslist": [
		"last 3 version",
		"> 1%",
		"not dead"
	]
}
